      "id": "link_2",
      "label": "Link 2"
    },
    {
      "type": "url",
      "id": "link_3",
      "label": "Link 3"
    },
    {
      "type": "url",
      "id": "link_4",
      "label": "Link 4"
    }
  ]
}
