{% schema %}
{
  "name": "V1 - Instagram Secondairy",
  "tag": "div",
  "class": "instagram-section programs-pink",
  "settings": [
    {
      "type": "textarea",
      "id": "title",
      "label": "Title",
      "default": "Volg onze socials"
    },
    {
