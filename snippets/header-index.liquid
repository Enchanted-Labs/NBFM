<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/swiper@8/swiper-bundle.min.css">
<script>
$('[data-click="accordion"]').click(function(){
if(!$(this).is('.open')){
$('[data-click="accordion"].open').each((i, item)=>{
item.click();
});
$(this).addClass('open');
}
else{
$(this).removeClass('open');
}
});
</script>
